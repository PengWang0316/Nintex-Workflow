{
  "name": "nintex-workflow",
  "version": "0.0.1",
  "description": "A web application for Nintex.",
  "main": "index.html",
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --mode development --open",
    "build": "set NODE_ENV=production & webpack --mode=production -p",
    "test": "jest",
    "testCoverage": "jest --coverage --runInBand",
    "testWatch": "jest --watch",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "keywords": [
    "Nintex"
  ],
  "author": "Kevin W.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PengWang0316/Nintex-Workflow.git"
  },
  "sideEffects": false,
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "e2e/*",
      "__tests__/models/TestData.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/tools/assetsTransformer.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testEnvironment": "jest-environment-jsdom-global",
    "testURL": "http://localhost",
    "verbose": true
  },
  "dependencies": {
    "axios": "^0.19.0",
    "tabulator-tables": "^4.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^5.0.2",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-environment-jsdom": "^24.0.0",
    "jest-environment-jsdom-global": "^1.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "style-loader": "^0.23.1",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "workbox-webpack-plugin": "^4.1.1"
  }
}
